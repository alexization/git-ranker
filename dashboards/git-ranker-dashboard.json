      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${prometheus_ds}" },
          "expr": "sum(rate(http_server_requests_seconds_count{application=\"git-ranker-api\", status=~\"5..\"}[5m])) / sum(rate(http_server_requests_seconds_count{application=\"git-ranker-api\"}[5m])) * 100 or vector(0)",
