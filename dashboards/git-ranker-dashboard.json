        { "datasource": { "type": "prometheus", "uid": "${prometheus_ds}" }, "expr": "increase(github_api_calls_total{application=\"git-ranker-api\", result=\"failure\"}[$__interval])", "legendFormat": "Failure", "refId": "B" }
