        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${prometheus_ds}" },
          "expr": "sum(rate(http_server_requests_seconds_count{application=\"git-ranker-api\"}[1m]))",
